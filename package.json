{
  "contributors": [{
    "name": "kai zhu",
    "email": "kaizhu256@gmail.com"
  }],
  "dependencies": {
    "connect": "2.11.x",
    "coveralls": "2.5.x",
    "graceful-fs": "2.0.x",
    "istanbul": "0.2.x",
    "phantomjs": "1.9.x",
    "sqlite3": "2.1.x",
    "uglify-js": "2.4.x",
    "utility2-external": "~2014.01.25"
  },
  "description": "utility2.js - standalone, browser test and code coverage framework for nodejs",
  "main": "utility2.js",
  "name": "utility2",
  "repository" : {
    "type" : "git",
    "url" : "https://github.com/kaizhu256/utility2.git"
  },
  "scripts": {
    "postinstall": "utility2=./utility2.js; external=./node_modules/utility2-external/public/utility2-external; $utility2 --minify $external.browser.css,$external.browser.js,$external.browser.lite.js,$external.browser.lite.js,$external.shared.js",
    "start": "./utility2.js --repl --serverPort random",
    "test": "utility2=./utility2.js; $utility2 --npm-test-mode start --npm-test-script $utility2 --timeout-default 8000 --npm-test-utility2 --timeout-exit 16000; $utility2 --coveralls; $utility2 --npm-test-mode end"
  },
  "stateDefault": {
    "coverallsUrl": "https://coveralls.io/r/kaizhu256/utility2"
  },
  "version": "2014.01.27-1"
}
