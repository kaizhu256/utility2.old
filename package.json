{
  "contributors": [{
    "name": "kai zhu",
    "email": "kaizhu256@gmail.com"
  }],
  "dependencies": {
    "connect": "2.13.x",
    "istanbul": "0.2.x",
    "phantomjs": "1.9.x",
    "sqlite3": "2.2.x",
    "utility2-external": "~2014.03.16"
  },
  "devDependencies": {
    "coveralls": "2.8.x"
  },
  "description": "utility2.js - standalone, browser test and code coverage framework for nodejs",
  "main": "utility2.js",
  "name": "utility2",
  "repository" : {
    "type" : "git",
    "url" : "https://github.com/kaizhu256/utility2.git"
  },
  "scripts": {
    "start": "./utility2.js --load-module utility2.js --mode-extra --mode-repl --serverPort random",
    "test": "./utility2.js --load-module utility2.js --mode-npm-test-utility2 && ./utility2.js --load-module utility2.js --mode-extra --mode-npm-test-utility2 && ($TRAVIS || cat tmp/lcov.info | node_modules/.bin/coveralls)"
  },
  "stateDefault": {
    "coverallsUrl": "https://coveralls.io/r/kaizhu256/utility2"
  },
  "version": "2014.03.16-1"
}
